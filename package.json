{
  "name": "slidy",
  "version": "0.5.19",
  "description": "Range slider component",
  "main": "index.js",
  "files": [
    "index.js",
    "picker.js",
    "index.css"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "contains": "latest",
    "define-state": "^1.x",
    "draggy": "^1.x",
    "emmy": "^5.x",
    "get-client-xy": "^1.0.1",
    "get-uid": "^1.0.1",
    "lifecycle-events": "^1.0.0",
    "mucss": "^1.0",
    "mumath": "^1.0.0",
    "sliced": "^0.0.5",
    "st8": "^1.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "resizable": "^1.1.0",
    "tst": "^1.0.6"
  },
  "style": "index.css",
  "scripts": {
    "watch": "watchify -r ./index.js:../ -r mucss -r assert -d -o ./bundle.js",
