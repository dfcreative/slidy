{
  "name": "slidy-demo",
  "version": "1.0.0",
  "description": "Slidy demo cases",
  "main": "index.js",
  "scripts": {
    "watch": "watchify -r slidy -r queried -r emmy/on -r web-audio-stats -d -o bundle.js",
    "build": "npm run build-js && npm run build-css",
    "build-js": "browserify -r slidy -r queried -r emmy/on -r web-audio-stats | ccjs - > bundle.js",
    "build-css": "postcss -u postcss-import -u autoprefixer index.css > bundle.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcreative/slidy.git"
  },
  "author": "Deema Ywanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/slidy/issues"
  },
  "homepage": "https://github.com/dfcreative/slidy",
  "dependencies": {
    "emmy": "^5.2.5",
    "normalize.css": "^3.0.3",
    "queried": "^1.4.1",
    "slidy": "^0.5.4",
    "web-audio-stats": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "browserify": ">=8",
    "closurecompiler": "*",
    "postcss": "^4.1.11",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2",
    "watchify": "^3.2.1"
  }
}
